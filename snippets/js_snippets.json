{
	"Pixel Pizza command": {
		"scope": "javascript",
		"prefix": [
			"command", 
			"cmd"
		],
		"body": [
			"const discord = require(\"discord.js\");",
			"${1:const PixelPizza = require(\"pixel-pizza\");\n}",
			"module.exports = {", 
			"\tname: \"${2:name}\",",
			"\tdescription: \"${3:description}\",",
			"\taliases: [${4:\"alias\"}],",
			"\targs: ${5|false,true|},",
			"\tminArgs: ${6/[^0-9]//g},",
			"\tmaxArgs: ${7/[^0-9]//g},",
			"\tusage: \"${8:usage}\",",
			"\tcooldown: ${9/[^0-9]//g},",
			"\tuserType: \"${10|all,vip,worker,teacher,staff,director|}\",",
			"\tneededPerms: [${11/[a-z]/${0:/upcase}/g}],",
			"\tpponly: ${12|false,true|},",
			"\tremoveExp: ${13|false,true|},",
			"\t$BLOCK_COMMENT_START*",
			"\t * ${14:Execute this command}",
			"\t * @param {discord.Message} message",
			"\t * @param {string[]} args",
			"\t * @param {${15|PixelPizza.PPClient,discord.Client|}} client",
			"\t * @returns {Promise<void>}",
			"\t $BLOCK_COMMENT_END",
			"\tasync execute(message, args, client) {",
			"\t\t$16",
			"\t}",
			"}"
		],
		"description": "Make a pixel pizza command"
	},
	"Pixel Pizza embed options": {
		"scope": "javascript",
		"prefix": [
			"embed", 
			"embedoptions"
		],
		"body": [
			"{",
			"\tcolor: \"${1:color}\",",
			"\ttitle: \"${2:title}\",",
			"\turl: \"${3:url}\",",
			"\tauthor: {",
			"\t\tname: \"${4:name}\",",
			"\t\ticon: \"${5:icon}\",",
			"\t\turl: \"${6:url}\",",
			"\t},",
			"\tdescription: \"${7:description}\",",
			"\tthumbnail: \"${8:thumbnail}\",",
			"\tfields: [",
			"\t\t{",
			"\t\t\tname: \"${9:name}\",",
			"\t\t\tvalue: \"${10:value}\",",
			"\t\t\tinline: ${11|false,true|},",
			"\t\t}${12:,}",
			"\t],",
			"\timage: \"${13:image}\",",
			"\ttimestamp: ${14|false,true|},",
			"\tfooter: {",
			"\t\ttext: \"${15:text}\",",
			"\t\ticon: \"${16:icon}\"",
			"\t}",
			"}"
		],
		"description": "Add embed options to current cursor position"
	}
}